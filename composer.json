{
    "name": "gamegos/nosql-apc",
    "description": "APC extension for gamegos/nosql library",
    "keywords": [
        "nosql",
        "key value",
        "cache"
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.5.0",
        "ext-apcu": "~4.0",
        "gamegos/nosql": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Gamegos\\NoSql\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "gamegos/php-code-sniffer": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "Gamegos\\NoSql\\Tests\\": [
                "tests/",
                "vendor/gamegos/nosql/tests/"
            ]
        }
    },
    "authors": [
        {
            "name": "Safak Ozpinar",
            "email": "safak@gamegos.com"
        }
    ]
}
